{
    "name": "Fashion Emissions Calculator",
    "description": "HackMIT",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "scripting", "tabs", "activeTab"],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
      ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
        "matches": ["https://*.shein.com/cart"],
        "js": ["cart-reader.js"]
        }
    ]
}